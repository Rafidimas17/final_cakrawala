<div class="modal fade" id="edit_modal" tabindex="-1" role="dialog"
aria-labelledby="exampleModalLabel" aria-hidden="true">
<div class="modal-dialog" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title" id="exampleModalLabel">Add Bank</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            <form action="/admin/bank?_method=PUT"  method="POST" enctype="multipart/form-data">
                <div class="form-group">
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">Name Bank</label>
                        <input type="text" class="form-control" id="namebank"  name="nameBank" placeholder="Enter name category" required>
                        <label for="recipient-name" class="col-form-label">Nomor Rekening</label>
                        <input type="text" class="form-control" id="nomorRekening"  name="nomorRekening" placeholder="Enter name category" required>
                        <label for="recipient-name" class="col-form-label">Name</label>
                        <input type="text" class="form-control" id="name"  name="name" placeholder="Enter name category" required>
                        <label for="recipient-name" class="col-form-label">Bukti Pembayaran:</label>
                        <input type="file" class="form-control"  name="image" placeholder="Enter name category">
                    </div>
                </div>

        </div>
        <div class="modal-footer">
            <input type="hidden" class="id" name="id" id="id"/>
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
        </form>
    </div>
</div>
</div>